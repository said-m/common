{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es2018","dom"],
    "moduleResolution": "node",
    "declaration": true,
    "strict": true,
    "typeRoots": ["node_modules/@types"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "pretty": true,
  }
}
